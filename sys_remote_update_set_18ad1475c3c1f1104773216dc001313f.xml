<name>sys_ui_page_3377bec9c38d31104773216dc001310a</name>
<payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update table="sys_ui_page"&gt;&lt;sys_ui_page action="INSERT_OR_UPDATE"&gt;&lt;category&gt;general&lt;/category&gt;&lt;client_script/&gt;&lt;description&gt;My eight jelly page - foreach test with an array / object&lt;/description&gt;&lt;direct&gt;false&lt;/direct&gt;&lt;endpoint/&gt;&lt;html&gt;&lt;![CDATA[&lt;?xml version="1.0" encoding="utf-8" ?&gt;
&lt;j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null"&gt;
	&lt;g:evaluate&gt;
		var colors = ["pink", "green", "violet", "indigo", "blue", "yellow", "orange", "white", "red"];
	&lt;/g:evaluate&gt;

	&lt;!-- &lt;g:evaluate var="jvar_colors" object="true" expression="['pink', 'green'];"/&gt; --&gt;

	&lt;j:forEach var="jvar_colors" items="${colors}"&gt;
		&lt;p&gt;Color: $[SP] &lt;span style="color:${jvar_colors}"&gt;${jvar_colors}&lt;/span&gt;&lt;/p&gt;
	&lt;/j:forEach&gt;

	&lt;j:forEach var="jvar_index" begin="0" end="20" step="2"&gt;
		${jvar_index}
	&lt;/j:forEach&gt;
&lt;/j:jelly&gt;]]&gt;&lt;/html&gt;&lt;name&gt;foreach_test&lt;/name&gt;&lt;processing_script/&gt;&lt;sys_class_name&gt;sys_ui_page&lt;/sys_class_name&gt;&lt;sys_created_by&gt;aakash.garg&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2023-09-01 17:14:57&lt;/sys_created_on&gt;&lt;sys_id&gt;3377bec9c38d31104773216dc001310a&lt;/sys_id&gt;&lt;sys_mod_count&gt;11&lt;/sys_mod_count&gt;&lt;sys_name&gt;foreach_test&lt;/sys_name&gt;&lt;sys_package display_value="Global" source="global"&gt;global&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_scope display_value="Global"&gt;global&lt;/sys_scope&gt;&lt;sys_update_name&gt;sys_ui_page_3377bec9c38d31104773216dc001310a&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;aakash.garg&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2023-09-03 13:47:16&lt;/sys_updated_on&gt;&lt;/sys_ui_page&gt;&lt;/record_update&gt;</payload>

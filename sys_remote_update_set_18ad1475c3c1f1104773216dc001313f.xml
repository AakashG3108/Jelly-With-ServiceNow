<name>sys_ui_page_2c4b4305c30171104773216dc00131b5</name>
<payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update table="sys_ui_page"&gt;&lt;sys_ui_page action="INSERT_OR_UPDATE"&gt;&lt;category&gt;general&lt;/category&gt;&lt;client_script&gt;&lt;![CDATA[function setUserFilter(){
	var asg = gel('assignment_group').value;
	var userLookup = gel('lookup.assigned_to');

	var users = [];
	var gr = new GlideRecord('sys_user_grmember');
	gr.addQuery('group', asg);
	gr.query();
	while(gr.next()){
		users.push(gr.user.toString());
	}

	userLookup.setAttribute('onclick', "mousePositionSave(event); reflistOpen('assigned_to', 'not', 'sys_user', '', 'false', 'QUERY:active=true', 'sys_idIN" + users + "', '')");
}]]&gt;&lt;/client_script&gt;&lt;description&gt;My eleventh jelly page - with form&lt;/description&gt;&lt;direct&gt;false&lt;/direct&gt;&lt;endpoint/&gt;&lt;html&gt;&lt;![CDATA[&lt;?xml version="1.0" encoding="utf-8" ?&gt;
&lt;j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null"&gt;
	&lt;g:ui_form&gt;
		Requestor: &lt;g:ui_reference id="requestor" name="requestor" table="sys_user" query="active=true" /&gt;&lt;br/&gt;
		&lt;g:evaluate&gt;
			var items = ["--None--", "Software", "Hardware", "Database"];
		&lt;/g:evaluate&gt;
		Category: 
		&lt;select&gt;
			&lt;j:forEach var="jvar_item" items="${items}"&gt;
			&lt;option value="${jvar_item.toLowerCase()}"&gt;${jvar_item}&lt;/option&gt;
			&lt;/j:forEach&gt;
		&lt;/select&gt;&lt;br/&gt;
		Assignment Group: &lt;g:ui_reference id="assignment_group" name="assignment_group" table="sys_user_group" query="active=true" completer="AJAXTableCompleter" onchange="setUserFilter()"/&gt;&lt;br/&gt;
		Assigned To: &lt;g:ui_reference id="assigned_to" name="assigned_to" table="sys_user" query="active=true" completer="AJAXTableCompleter"/&gt;&lt;br/&gt;
		Short Description: &lt;input type="text" id="short_description" name="short_description" /&gt;&lt;br/&gt;
		&lt;!-- &lt;textarea rows="10" cols="100"&gt;&lt;/textarea&gt;&lt;br/&gt; --&gt;
		&lt;div class="buttons"&gt;
		&lt;button type="submit"&gt;Submit&lt;/button&gt;
		&lt;button type="reset"&gt;Reset&lt;/button&gt;
		&lt;/div&gt;
	&lt;/g:ui_form&gt;
&lt;/j:jelly&gt;]]&gt;&lt;/html&gt;&lt;name&gt;jelly_ui_form&lt;/name&gt;&lt;processing_script&gt;&lt;![CDATA[var gr = new GlideRecord("incident");
gr.initialize();
gr.setValue('caller_id', requestor);
gr.setValue('category', category);
gr.setValue('assignment_group', assignment_group);
gr.setValue('assigned_to', assigned_to);
gr.setValue('short_description', short_description);
gr.insert();

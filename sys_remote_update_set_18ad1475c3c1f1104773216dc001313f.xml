<name>sys_ui_page_1030a289c34d31104773216dc00131b2</name>
<payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update table="sys_ui_page"&gt;&lt;sys_ui_page action="INSERT_OR_UPDATE"&gt;&lt;category&gt;general&lt;/category&gt;&lt;client_script/&gt;&lt;description&gt;My fourth jelly page - with gliderecord example&lt;/description&gt;&lt;direct&gt;false&lt;/direct&gt;&lt;endpoint/&gt;&lt;html&gt;&lt;![CDATA[&lt;?xml version="1.0" encoding="utf-8" ?&gt;
&lt;j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null"&gt;
	&lt;g2:evaluate&gt;
		var gr = new GlideRecord("problem");
		gr.addQuery('active', 'true');
		gr.query();
		gr;
	&lt;/g2:evaluate&gt;

	&lt;table border="1"&gt;
		&lt;tr&gt;&lt;td colspan="2"&gt;Active Problems&lt;/td&gt;&lt;/tr&gt;
		&lt;tr&gt;
			&lt;th&gt;Number&lt;/th&gt;
			&lt;th&gt;Short Description&lt;/th&gt;
		&lt;/tr&gt;
	&lt;j2:while test="$[gr.next()]"&gt;
		&lt;tr&gt;
		&lt;td&gt;$[gr.number]&lt;/td&gt;
		&lt;td&gt;$[gr.short_description]&lt;/td&gt;
		&lt;/tr&gt;
	&lt;/j2:while&gt;
	&lt;/table&gt;

	&lt;style&gt;
		td, th{
			padding: 10px;
		}
	&lt;/style&gt;
&lt;/j:jelly&gt;]]&gt;&lt;/html&gt;&lt;name&gt;gliderecord_demo&lt;/name&gt;&lt;processing_script/&gt;&lt;sys_class_name&gt;sys_ui_page&lt;/sys_class_name&gt;&lt;sys_created_by&gt;aakash.garg&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2023-09-01 15:33:07&lt;/sys_created_on&gt;&lt;sys_id&gt;1030a289c34d31104773216dc00131b2&lt;/sys_id&gt;&lt;sys_mod_count&gt;9&lt;/sys_mod_count&gt;&lt;sys_name&gt;gliderecord_demo&lt;/sys_name&gt;&lt;sys_package display_value="Global" source="global"&gt;global&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_scope display_value="Global"&gt;global&lt;/sys_scope&gt;&lt;sys_update_name&gt;sys_ui_page_1030a289c34d31104773216dc00131b2&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;aakash.garg&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2023-09-01 15:42:39&lt;/sys_updated_on&gt;&lt;/sys_ui_page&gt;&lt;/record_update&gt;</payload>

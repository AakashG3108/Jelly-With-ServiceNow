<name>sys_ui_page_ebcf9e49c34d31104773216dc001318d</name>
<payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update table="sys_ui_page"&gt;&lt;sys_ui_page action="INSERT_OR_UPDATE"&gt;&lt;category&gt;general&lt;/category&gt;&lt;client_script/&gt;&lt;description&gt;My third jelly page - with some jexl expressions&lt;/description&gt;&lt;direct&gt;false&lt;/direct&gt;&lt;endpoint/&gt;&lt;html&gt;&lt;![CDATA[&lt;?xml version="1.0" encoding="utf-8" ?&gt;
&lt;j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null"&gt;
	&lt;j:set var="jvar_age" value="18"&gt;
	&lt;/j:set&gt;

	&lt;j:if test="${jvar_age lt 18}"&gt;
		Sorry, You are not eligible for driving licence. As we got your age is ${jvar_age}.
	&lt;/j:if&gt;
	
	&lt;j:if test="${jvar_age ge 18}"&gt;
		Hey, You are eligible for driving licence. As we got your age is ${jvar_age}.
	&lt;/j:if&gt;
&lt;/j:jelly&gt;]]&gt;&lt;/html&gt;&lt;name&gt;jexl_expressions&lt;/name&gt;&lt;processing_script/&gt;&lt;sys_class_name&gt;sys_ui_page&lt;/sys_class_name&gt;&lt;sys_created_by&gt;aakash.garg&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2023-09-01 15:31:27&lt;/sys_created_on&gt;&lt;sys_id&gt;ebcf9e49c34d31104773216dc001318d&lt;/sys_id&gt;&lt;sys_mod_count&gt;1&lt;/sys_mod_count&gt;&lt;sys_name&gt;jexl_expressions&lt;/sys_name&gt;&lt;sys_package display_value="Global" source="global"&gt;global&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_scope display_value="Global"&gt;global&lt;/sys_scope&gt;&lt;sys_update_name&gt;sys_ui_page_ebcf9e49c34d31104773216dc001318d&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;aakash.garg&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2023-09-01 15:32:19&lt;/sys_updated_on&gt;&lt;/sys_ui_page&gt;&lt;/record_update&gt;</payload>

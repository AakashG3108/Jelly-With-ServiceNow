<name>sys_ui_page_ab23ba0dc38d31104773216dc0013172</name>
<payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update table="sys_ui_page"&gt;&lt;sys_ui_page action="INSERT_OR_UPDATE"&gt;&lt;category&gt;general&lt;/category&gt;&lt;client_script/&gt;&lt;description&gt;My seventh jelly page - with swtich / case&lt;/description&gt;&lt;direct&gt;false&lt;/direct&gt;&lt;endpoint/&gt;&lt;html&gt;&lt;![CDATA[&lt;?xml version="1.0" encoding="utf-8" ?&gt;
&lt;j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null"&gt;
	&lt;j:set var="jvar_name" value="Aakash"&gt;
	&lt;/j:set&gt;

	&lt;j:switch on="${jvar_name}"&gt;
		&lt;j:case value="Richa"&gt;
			Welcome, Richa!
		&lt;/j:case&gt;
		&lt;j:case value="Atul"&gt;
			Welcome, Atul!
		&lt;/j:case&gt;
		&lt;j:default&gt;
			Welcome, ${jvar_name}!
		&lt;/j:default&gt;
	&lt;/j:switch&gt;
&lt;/j:jelly&gt;]]&gt;&lt;/html&gt;&lt;name&gt;switch_case&lt;/name&gt;&lt;processing_script/&gt;&lt;sys_class_name&gt;sys_ui_page&lt;/sys_class_name&gt;&lt;sys_created_by&gt;aakash.garg&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2023-09-01 16:56:05&lt;/sys_created_on&gt;&lt;sys_id&gt;ab23ba0dc38d31104773216dc0013172&lt;/sys_id&gt;&lt;sys_mod_count&gt;0&lt;/sys_mod_count&gt;&lt;sys_name&gt;switch_case&lt;/sys_name&gt;&lt;sys_package display_value="Global" source="global"&gt;global&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_scope display_value="Global"&gt;global&lt;/sys_scope&gt;&lt;sys_update_name&gt;sys_ui_page_ab23ba0dc38d31104773216dc0013172&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;aakash.garg&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2023-09-01 16:56:05&lt;/sys_updated_on&gt;&lt;/sys_ui_page&gt;&lt;/record_update&gt;</payload>
